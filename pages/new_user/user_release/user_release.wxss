page {
  background: #e6e8ea;
}

.one_level_nav {
  display: flex;
  justify-content: space-around;
  font-size: 14px;
  color: #a3a3a3;
  background: #fff;
  padding: 20rpx;
}

.one_level_nav .ac {
  color: #509ad2;
  border-bottom: 1px solid #509ad2;
}

.info_ul {
  font-size: 14px;
  color: #333;
  margin: 18rpx 0;
 
  background: #fff;

}

.info_ul .a3a3a3 {
  color: #a3a3a3;
}

.info_ul .info_ul_item {
  padding: 24rpx;
  display: flex;
  border-bottom: 1px solid #e6e8ea;
}

.info_ul .info_ul_item > view:first-child {
  width: 86rpx;
  height: 86rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 40rpx;
}

.info_ul .info_ul_item view:first-child image {
  width: 100%;
  height: 100%;
}

.info_ul .info_ul_item view:last-child {
  width: 80%;
}

.release_images_warp{
  display: flex;
}
.release_images_warp > view{
  width: 124rpx !important;
  height:124rpx;
  margin-right: 4rpx;
}
.release_images_warp > view >image{
  width: 100%;
  height:100%;
}