.header {
  background: #509ad2;
  padding: 36rpx 0 10rpx 0;
}

.header > view:last-child {
  display: flex;
  color: #fff;
  font-size: 12px;
  margin-top: 20rpx;
  margin-left: 34rpx;
}

.header .search {
  width: 90%;
  margin: 0 auto;
  height: 80rpx;
  line-height: 80rpx;
  color: #333;
  background: #fff;
  border-radius: 6px;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.header .search text {
  color: #509ad2;
  font-size: 14px;
}

.header .search input {
  font-size: 12px;
  width: 70%;
}

.category_warp {
  height: 100%;
  display: flex;
}

.category_warp .category_warp_nav {
  height: 100%;
  width: 160rpx;
  display: flex;
  flex-direction: column;
}

.category_warp .category_warp_nav >view {
  line-height: 90rpx;
  text-align: center;
  width: 100%;
}

.category_warp .category_warp_nav .ac {
  background: #f5f5f5;
}

.category_warp .category_warp_two_three_category {
  width: 590rpx;
  background: #f5f5f5;
  padding-left: 20rpx;
  box-sizing: border-box;
}

.category_warp .category_warp_two_three_category .title{
  line-height: 36px;
}
.category_warp .category_warp_two_three_category .ul{
  background: #fff;
  display: flex;
  flex-wrap:wrap;
}
.category_warp .category_warp_two_three_category .ul .li{
  display: flex;
 flex-direction: column;
 align-items: center;
 width: 33%;
}
.category_warp .category_warp_two_three_category .ul .li>span
{
  line-height: 34px;
}
.category_warp .category_warp_two_three_category .ul image{
  width:150rpx;
  height: 150rpx;
}
